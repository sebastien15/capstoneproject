    <div class="loginModal">
        <div id="closer">
            <span id="closeL">X</span>
        </div>
        <div class="header">
            <h3>Sign in</h3>
        </div>
        <div class="body">
            <form id="signInForm">
                <div class="errorBox disNone" id="unameErrorBox">
                    <p>Please Enter Password</p>
                    <span class="closer" onclick="this.parentNode.style.display='none'">X</span>
                </div>
                <input type="text" class="signInInputs" id="uname" placeholder="Username">
                <div class="errorBox disNone" id="passErrorBox">
                    <p>Please Enter Password</p>
                    <span class="closer" onclick="this.parentNode.style.display='none'">X</span>
                </div>
                <input type="password" class="signInInputs" id="pass" placeholder="password">
